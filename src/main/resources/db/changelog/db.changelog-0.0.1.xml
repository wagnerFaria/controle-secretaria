<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xmlns:ext="http://www.liquibase.org/xml/ns/dbchangelog-ext"
                   xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog-ext http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-ext.xsd http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd">

    <changeSet id="CREATE TABLE servidor" author="wagner">
        <createTable tableName="servidor">
            <column name="id" type="bigint" autoIncrement="true">
                <constraints primaryKey="true"/>
            </column>
            <column name="nome" type="varchar">
                <constraints nullable="false"/>
            </column>
            <column name="unidade_de_lotacao" type="varchar">
                <constraints nullable="false"/>
            </column>
            <column name="data_nascimento" type="date">
                <constraints nullable="false"/>
            </column>
            <column name="matricula" type="varchar">
                <constraints nullable="false"/>
            </column>
            <column name="cpf" type="varchar">
                <constraints nullable="false"/>
            </column>
            <column name="registro_geral" type="varchar">
                <constraints nullable="false"/>
            </column>
            <column name="telefone" type="varchar">
                <constraints nullable="false"/>
            </column>
        </createTable>
    </changeSet>
</databaseChangeLog>